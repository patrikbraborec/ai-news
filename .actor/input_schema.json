{
    "title": "CheerioCrawler Template",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "newsletterUrls": {
            "title": "Newsletter URLs",
            "type": "array",
            "description": "URLs to start with.",
            "editor": "requestListSources",
            "prefill": [
                {
                    "url": "https://news.smol.ai"
                }
            ]
        },
        "mode": {
            "title": "Mode",
            "type": "string",
            "description": "Mode to run the actor in.",
            "enum": ["basic", "full"],
            "default": "basic"
        }
    }
}
